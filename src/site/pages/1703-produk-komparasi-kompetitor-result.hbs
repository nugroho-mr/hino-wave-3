<script>
    document.addEventListener( 'DOMContentLoaded', function () {
        let elms = document.getElementsByClassName("splide--product-comparison");
        let splideElms = [];
        for (var i = 0, len = elms.length; i < len; i++) {
            splideElms[i] = new Splide(elms[i], {
                perPage: 4,
                arrows: false,
                pagination: false,
                gap: '50px',
                drag: false,
                breakpoints: {
                    768: {
                        perPage: 2,
                        gap: '30px',
                    },
                }
            }).mount();
        }

        const prevBtn = document.querySelector('.komparasi__prev');
        prevBtn.addEventListener('click', function() {
            splideElms.forEach(function (item, index) {
                item.go('-');
            });
        });
        const nextBtn = document.querySelector('.komparasi__next');
        nextBtn.addEventListener('click', function() {
            splideElms.forEach(function (item, index) {
                item.go('+');
            });            
        });

        splideElms[0].on('move', function(nextIndex, prevIndex) {
            if( nextIndex == ( Math.ceil(splideElms[0].length / splideElms[0].options.perPage) ) ) {
                nextBtn.classList.add('d-none');
            } else if( nextIndex <= 0 ) { 
                prevBtn.classList.add('d-none');
            } else {
                nextBtn.classList.remove('d-none');
                prevBtn.classList.remove('d-none');
            }
        })
    });
</script>